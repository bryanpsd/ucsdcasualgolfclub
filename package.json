{
	"name": "ucsdcasualgolfclub",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"start": "pnpm dev",
		"dev": "astro dev --open",
		"build": "astro build",
		"check": "astro check",
		"preview": "astro preview",
		"astro": "astro",
		"test": "pnpm test:unit && pnpm test:e2e",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:codegen": "playwright codegen http://localhost:4321",
		"format": "pnpm format:prettier && pnpm format:biome",
		"format:biome": "biome format --write .",
		"format:prettier": "prettier --write \"**/*.astro\"",
		"lint": "pnpm lint:biome && pnpm lint:eslint",
		"lint:biome": "biome lint .",
		"lint:eslint": "eslint \"src/**/*.astro\"",
		"lint:fix": "pnpm lint:biome --write && pnpm lint:eslint --fix",
		"cf-export": "contentful space export --config src/services/contentful/export-config.json --management-token $CONTENTFUL_DELIVERY_TOKEN --space-id $CONTENTFUL_SPACE_ID",
		"cf-generate-types": "cf-content-types-generator src/services/contentful/export.json --v10 --typeguard --out src/types/contentful",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.5",
		"@astrojs/netlify": "^6.6.0",
		"@astrojs/react": "^4.4.2",
		"@astrojs/sitemap": "^3.6.0",
		"@contentful/rich-text-html-renderer": "^17.0.0",
		"@contentful/rich-text-react-renderer": "^16.0.1",
		"@contentful/rich-text-types": "^17.2.2",
		"@google-cloud/recaptcha-enterprise": "^6.1.0",
		"@netlify/blobs": "^10.4.1",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-menubar": "^1.1.6",
		"@radix-ui/react-navigation-menu": "^1.2.5",
		"@radix-ui/react-select": "^2.2.2",
		"@svgr/webpack": "^8.1.0",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vanilla-extract/css": "^1.17.1",
		"@vanilla-extract/recipes": "^0.5.5",
		"@vanilla-extract/sprinkles": "^1.6.3",
		"astro": "^5.15.8",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"contentful": "^11.4.4",
		"contentful-management": "^11.52.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.5.0",
		"node-fetch": "^3.3.2",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.56.3",
		"react-icons": "^5.5.0",
		"react-toastify": "^11.0.5",
		"recaptcha-v3": "^1.11.3",
		"vite-tsconfig-paths": "^5.1.4",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@playwright/test": "^1.57.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"@vanilla-extract/vite-plugin": "^3.9.5",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/ui": "^4.0.14",
		"axe-core": "^4.10.3",
		"cf-content-types-generator": "^2.15.5",
		"eslint": "^9.39.1",
		"eslint-plugin-astro": "^1.5.0",
		"happy-dom": "^20.0.10",
		"jsdom": "^27.2.0",
		"playwright": "^1.57.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"typescript": "^5.8.3",
		"vite-plugin-svgr": "^4.3.0",
		"vitest": "^4.0.14"
	}
}
